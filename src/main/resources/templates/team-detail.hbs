{{#partial "content"}}

   <p>Team Details:</p>

   <p>{{ team.teamName }}</p>
   <h5>Created At:</h5>
   <p> {{ team.createdAt }}</p>
   <h5>Team status:</h5>
       {{#if team.published }}
           <span class="glyphicon glyphicon-star" aria-hidden="true"></span>  Published
       {{else}}
           <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span> Draft
       {{/if}}

       <a href="/teams/{{team.id}}/update">Edit this Team name</a><br>

   </p>

{{/partial}}

{{> layout.hbs}}