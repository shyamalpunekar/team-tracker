{{#partial "content"}}

    <p>Member Details:</p>

    <p>{{ member.memberName }}</p>
    <h5>Created At:</h5>
    <p> {{ member.createdAt }}</p>
    <h5>Team status:</h5>
    {{#if member.published }}
        <span class="glyphicon glyphicon-star" aria-hidden="true"></span>  Published
    {{else}}
        <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span> Draft
    {{/if}}

    {{!-- <a href="/members/{{member.id}}/update">Edit this Member</a><br> --}}


    <a href="/members/{{member.id}}/update">Edit this Member</a><br>
    <a href="/members/{{member.id}}/delete">DELETE this Member (no undo)</a>

    </p>

{{/partial}}



